{
    "compilerOptions": {
        "lib": [ "es2015" ],
        "module": "commonjs",
        "target": "es5",
        "noImplicitAny": false,
        "sourceMap": false
    },
    "exclude": [
        "node_modules"
    ],
    "typeRoots": [
        "node_modules/@types"
    ],
    "paths": {
        "*" : ["src/types/*"]
    }
}
